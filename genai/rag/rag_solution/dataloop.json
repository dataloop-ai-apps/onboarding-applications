      "Text"
    ]
  },
  "metadata": {
    "system": {
      "systemCompute": true
    }
  },
  "dependencies": [
    {
      "name": "rag-template"
    },
    {
      "name": "dataset-text-recipes"
    },
    {
      "name": "text-embeddings-3"
    },
    {
      "name": "chat-completion"
    }
  ],
  "variables": {
    "dataset_id": "dataset-text-recipes.dataset.dataset-text-recipes",
    "fs_id": "dataset-text-recipes.dataset.dataset-text-recipes",
    "model": "text-embeddings-3.model.openai-gpt-4o",
    "embedder": "chat-completion.model.openai-prompt-embeddings-3l"
