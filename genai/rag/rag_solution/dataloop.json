{
  "displayName": "RAG Onboarding Solution",
  "name": "rag-onboarding-solution",
  "description": "RAG Onboarding Solution",
  "scope": "public",
  "version": "1.0.30",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/onboarding-applications.git",
    "gitTag": "1.0.30"
  },
  "attributes": {
    "Provider": "Dataloop",
    "Category": "Application",
    "Application Type": "Other",
